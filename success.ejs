    .container h1 {
      font-size: 24px; 
      color: #333; 
      margin-bottom: 10px; 
    }

    
    .container p {
      font-size: 16px; 
      color: #666; 
      margin-bottom: 20px;
    }


    .container a {
      font-size: 16px;
      color: #007bff; 
      text-decoration: none; 
    }

    .container a:hover {
      text-decoration: underline; 
    }

